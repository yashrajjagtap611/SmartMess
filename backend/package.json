{
  "name": "SmartMess-backend",
  "version": "1.0.0",
  "description": "SmartMess backend server",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "insert-users": "ts-node src/scripts/insertUsers.ts",
    "create-test-user": "ts-node src/scripts/createTestUser.ts",
    "update-passwords": "ts-node src/scripts/updateUserPasswords.ts",
    "check-users": "ts-node src/scripts/checkUsers.ts",
    "test-server-connection": "ts-node src/scripts/testServerConnection.ts",
    "insert-users-with-subscription": "ts-node src/scripts/insertUsersWithSubscription.ts",
    "check-database": "ts-node src/scripts/checkDatabase.ts",
    "migrate-mess-membership": "node scripts/migrate-mess-membership.js",
    "test-db": "node test-db-connection.js",
    "test-api": "node test-members-api.js",
    "test-system": "node test-system-integration.js",
    "test-db-connection": "node test-db-connection.js",
    "test:rejoin": "ts-node scripts/testing/testRejoinFlow.ts",
    "test:rejoin-issue": "ts-node scripts/testing/testRejoinIssue.ts",
    "test:rejoin-all": "ts-node scripts/testing/runAllRejoinTests.ts",
    "test:quick-fix": "ts-node scripts/testing/testQuickFix.ts",
    "test:admin-endpoint": "ts-node test-admin-endpoints.js",
    "clean-duplicates": "ts-node scripts/database/cleanDuplicateNotifications.ts",
    "fix-notification-index": "ts-node scripts/database/fixNotificationIndex.ts",
    "seed:users": "node scripts/seed-users-from-json.js --file scripts/data/users.sample.json"
  },
  "dependencies": {
    "@types/qrcode": "^1.5.6",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.10",
    "qrcode": "^1.5.4",
    "razorpay": "^2.9.6",
    "socket.io": "^4.8.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.23",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/express-validator": "^2.20.33",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/methods": "^1.1.4",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.19.130",
    "@types/nodemailer": "^6.4.7",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^2.0.16",
    "@types/triple-beam": "^1.3.5",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
